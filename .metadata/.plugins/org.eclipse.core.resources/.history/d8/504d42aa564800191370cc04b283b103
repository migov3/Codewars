package integerSquareRoot;

import java.util.ArrayList;

public class integerSquareRoot {

	public static void main(String[] args) {
		String n = "922337203685412321377580"; //458
		System.out.println(integerToSquareRoot(n));
	}

	public static String integerToSquareRoot(String n) {
		String res = "";
		if(n.length() < 19) {
			res = String.valueOf((int)Math.sqrt(Double.parseDouble(n)));
		}else {
			for(int i = 0; i < 16; i++) {
				res = res + n.charAt(i);
			}
			int root = (int)Math.floor(Math.sqrt(Long.parseLong(res)));
			res = String.valueOf(root * Math.pow(10, n.length()));
		}
		return res;
	}
}
